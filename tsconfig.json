{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "types": ["node", "ts-node/register"],
    "module": "CommonJS",
    "target": "ESNext",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strict": true
  }
}
